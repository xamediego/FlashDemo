          <Label fx:id="unfinishedNew" text="10" />
          <Label text="|" />
          <Label fx:id="reviews" text="20" />
         </children>
      </HBox>
    <Button maxHeight="20.0" maxWidth="20.0" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" HBox.hgrow="NEVER">
